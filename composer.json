{
    "name": "kaizen/laravel-oauth-client",
    "description": "Laravel Socialite provider for Kaizen OAuth authentication",
    "keywords": ["laravel", "socialite", "oauth", "kaizen", "authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kaizen Team",
            "email": "contact@kaizenmc.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/socialite": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Kaizen\\OAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaizen\\OAuth\\KaizenServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
