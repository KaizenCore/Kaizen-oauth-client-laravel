{
    "name": "kaizencore/laravel-oauth-client",
    "description": "Laravel Socialite provider for Kaizen OAuth authentication",
    "keywords": ["laravel", "socialite", "oauth", "kaizen", "authentication", "minecraft"],
    "license": "MIT",
    "homepage": "https://github.com/KaizenCore/Kaizen-oauth-client-laravel",
    "authors": [
        {
            "name": "Kaizen Team",
            "email": "contact@kaizenmc.fr",
            "homepage": "https://kaizencore.tech"
        }
    ],
    "support": {
        "issues": "https://github.com/KaizenCore/Kaizen-oauth-client-laravel/issues",
        "source": "https://github.com/KaizenCore/Kaizen-oauth-client-laravel"
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/socialite": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kaizen\\OAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaizen\\OAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaizen\\OAuth\\KaizenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
